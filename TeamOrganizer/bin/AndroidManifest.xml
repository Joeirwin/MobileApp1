<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.activity.teamorganizer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.activity.teamorganizer.TasksActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.activity.teamorganizer.NewTaskActivity"
            android:label="@string/title_activity_new_task" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.ViewSingleTask"
            android:label="@string/title_activity_view_single_task" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.SingleActivityView"
            android:label="@string/title_activity_single_activity_view" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.UserActivity"
            android:label="@string/title_activity_user" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.NewUserActivity"
            android:label="@string/title_activity_new_user" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.SingleUserViewActivity"
            android:label="@string/title_activity_single_user_view" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.SingleUserActivity"
            android:label="@string/title_activity_single_user" >
        </activity>
        <activity
            android:name="com.activity.teamorganizer.Teste"
            android:label="@string/title_activity_teste" >
        </activity>
    </application>

</manifest>
